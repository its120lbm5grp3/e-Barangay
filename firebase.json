{
  "hosting": {
    "public": "workspace",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/LogRegPage/",
        "type": 301
      },
      {
        "source": "/UserPage",
        "destination": "/UserPage/",
        "type": 301
      },
      {
        "source": "/AdminPage",
        "destination": "/AdminPage/",
        "type": 301
      },
      {
        "source": "/LogRegPage",
        "destination": "/LogRegPage/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
