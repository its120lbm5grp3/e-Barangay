{
  "hosting": {
    "public": "workspace",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/Log-Reg Page/",
        "type": 301
      },
      {
        "source": "/UserPage",
        "destination": "/UserPage/",
        "type": 301
      },
      {
        "source": "/AdminPage",
        "destination": "/AdminPage/",
        "type": 301
      },
      {
        "source": "/Log-Reg Page",
        "destination": "/Log-Reg Page/",
        "type": 301
      }
    ],
    "rewrites": []
  }
}
